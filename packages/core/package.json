{
  "name": "@nillion/core",
  "version": "0.1.1",
  "type": "module",
  "browser": "dist/index.js",
  "types": "src/index.ts",
  "scripts": {
    "proto:generate": "protoc --plugin=../../node_modules/.bin/protoc-gen-ts_proto --ts_proto_out=src proto/nilchain.proto",
